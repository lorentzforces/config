#!/bin/sh

xrandr --output DP-0 --off --output DP-1 --off --output HDMI-0 --off --output DP-2 --primary --mode 2560x1440 --rate 143.97 --pos 1440x560 --rotate normal --brightness 1.0 --gamma 1:1:1 --output DP-3 --off --output HDMI-1 --mode 1920x1080 --pos 4000x740 --rotate normal --output DP-4 --mode 2560x1440 --rate 143.97 --pos 0x0 --rotate left --brightness 1.0 --gamma 1:1:1 --output DP-5 --off
