{
  "editor.fontFamily": "Hack",
  "window.titleBarStyle": "custom",

  "editor.wordWrap": "on",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,

  "terminal.integrated.shell.windows": "C:\\Windows\\Sysnative\\WindowsPowerShell\\v1.0\\powershell.exe",
  "window.menuBarVisibility": "toggle",

  "editor.minimap.enabled": false,

  "powershell.integratedConsole.showOnStartup": false,
  "workbench.activityBar.visible": false,

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
  },

  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "editor.acceptSuggestionOnEnter": "off",

  "files.associations":{
    "*.icml": "xsl",
    "*.fo": "xsl",
    ".npmrc": "ini",
  },

  "[plaintext]": {
    "editor.quickSuggestions": {
      "other": false,
      "comments": false,
      "strings": false
    }
  },

  "[git-commit]": {
    "editor.quickSuggestions": {
      "other": false,
      "comments": false,
      "strings": false
    }
  },
  "vim.useSystemClipboard": true,
  "java.errors.incompleteClasspath.severity": "ignore",

  // colors
  "workbench.colorCustomizations": {
    // base colors
    "focusBorder": "#ffffff",
    "foreground": "#e87e7c",
    "errorForeground": "#b1a400",
    "selection.background": "#555555",
    // everything else
    "activityBar.background": "#000000",
    "activityBar.foreground": "#f39745",
    "activityBar.inactiveForeground": "#808080",
    "button.background": "#5b1b1b",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#d60010",
    "badge.background": "#bbbbbb",
    "badge.foreground": "#101010",
    "diffEditor.insertedTextBackground": "#294436",
    "diffEditor.removedTextBackground": "#301b1b",
    "dropdown.background": "#101010",
    "dropdown.foreground": "#e87e7c",
    "dropdown.listBackground": "#101010",
    "editor.background": "#101010",
    "editor.foreground": "#e87e7c",
    "editor.lineHighlightBackground": "#000000",
    "editor.selectionBackground": "#555555",
    "editor.selectionForeground": "#1f1f1f",
    "editor.selectionHighlightBackground": "#000000",
    "editor.selectionHighlightBorder": "#ffffff",
    "editorBracketMatch.border": "#ffffff",
    "editorCursor.foreground": "#bbbbbb",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGutter.addedBackground": "#447152",
    "editorGutter.background": "#000000",
    "editorGutter.deletedBackground": "#5b1b1b",
    "editorGutter.modifiedBackground": "#43698d",
    "editorLineNumber.foreground": "#808080",
    "editorRuler.foreground": "#1f1f1f",
    "editorLineNumber.activeForeground": "#e87e7c",
    "gitDecoration.addedResourceForeground": "#447152",
    "gitDecoration.modifiedResourceForeground": "#43698d",
    "gitDecoration.conflictingResourceForeground": "#b1a400",
    "gitDecoration.deletedResourceForeground": "#5b1b1b",
    "gitDecoration.ignoredResourceForeground": "#808080",
    "gitDecoration.untrackedResourceForeground": "#d60010",
    "input.background": "#555555",
    "inputOption.activeBorder": "#d60010",
    "inputValidation.infoForeground": "#ffffff",
    "inputValidation.warningForeground": "#1f1f1f",
    "inputValidation.errorForeground": "#ffffff",
    "inputValidation.infoBackground": "#43698d",
    "inputValidation.warningBackground": "#b1a400",
    "inputValidation.errorBackground": "#5b1b1b",
    "list.activeSelectionBackground": "#555555",
    "list.focusBackground": "#555555",
    "list.hoverBackground": "#101010",
    "list.inactiveSelectionBackground": "#555555",
    "panel.background": "#101010",
    "panelTitle.activeForeground": "#f39745",
    "panelTitle.inactiveForeground": "#808080",
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#55555580",
    "scrollbarSlider.hoverBackground": "#55555580",
    "scrollbarSlider.background": "#55555580",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#e87e7c",
    "sideBarSectionHeader.background": "#1f1f1f",
    "sideBarSectionHeader.foreground": "#f39745",
    "sideBarTitle.foreground": "#f39745",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#e87e7c",
    "statusBar.noFolderBackground": "#000000",
    "tab.activeForeground": "#f39745",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#808080",
    // terminal colors
    "terminal.background": "#101010",
    "terminal.foreground": "#e87e7c",
    // these are backwards - fg = caret, bg = letter
    "terminalCursor.background": "#1f1f1f",
    "terminalCursor.foreground": "#bbbbbb",
    // these following colors correspond to ANSI 0-15
    "terminal.ansiBlack": "#101010",
    "terminal.ansiRed": "#5b1b1b",
    "terminal.ansiGreen": "#447152",
    "terminal.ansiYellow": "#413c00",
    "terminal.ansiBlue": "#43698d",
    "terminal.ansiMagenta": "#f39745",
    "terminal.ansiCyan": "#bb83d1",
    "terminal.ansiWhite": "#e87e7c",
    "terminal.ansiBrightBlack": "#808080",
    "terminal.ansiBrightRed": "#d60010",
    "terminal.ansiBrightGreen": "#6fdbb2",
    "terminal.ansiBrightYellow": "#b1a400",
    "terminal.ansiBrightBlue": "#00b8ff",
    "terminal.ansiBrightMagenta": "#f39745",
    "terminal.ansiBrightCyan": "#bb83d1",
    "terminal.ansiBrightWhite": "#ffffff",
  },
  "editor.tokenColorCustomizations": {
    "keywords": "#bb83d1",
    "functions": "#d60010",
    "variables": "#e87e7c",
    "comments": "#808080",
    "types": "#f39745",
    "numbers": "#6fdbb2",
    "strings": "#6fdbb2",
    "textMateRules": [
      {
        "name": "raw/default source",
        "scope": [
          "source",
          "text",
        ],
        "settings": {
          "foreground": "#e87e7c",
        }
      },
      {
        "name": "keywords",
        "scope": [
          "storage.modifier",
          "keyword.operator.new",
          "keyword.other",
          "variable.language.this",
        ],
        "settings": {
          "foreground": "#bb83d1",
        }
      },
      {
        "name": "types",
        "scope": [
          "entity.name",
          "entity.name.tag",
          "entity.other.inherited-class",
          "storage.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "constant.other.table-name.sql",
          "constant.other.database-name.sql",
        ],
        "settings": {
          "foreground": "#f39745",
          "fontStyle": "italic",
        }
      },
      {
        "name": "punctuation and operators",
        "scope": [
          "keyword.operator",
          "punctuation",
          "keyword.control.ternary",
          "constant.character.escape",
          "storage.type.function.arrow",
          "meta.brace.round",
          "meta.brace.square",
          "meta.brace.angle",
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html",
        ],
        "settings": {
          "foreground": "#ffffff",
        }
      },
      {
        "name": "comments",
        "scope": [
          "comment",
          "comment.line.number-sign.git-commit",
        ],
        "settings": {
          "foreground": "#808080",
          "fontStyle": "italic",
        }
      },
      {
        "name": "functions and methods",
        "scope": [
          "entity.name.function",
          "meta.method.declaration",
          "support.function.git-rebase"
        ],
        "settings": {
          "foreground": "#d60010",
          "fontStyle": "",
        }
      },
      {
        "name": "variables",
        "scope": [
          "variable",
          "variable.parameter",
          "entity.other.attribute-name",
          "constant.other.key",
          "support.variable.property",
          "support.type.property-name.json",
          "meta.object-literal.key",
        ],
        "settings": {
          "foreground": "#e87e7c",
        }
      },
      {
        "name": "language built-ins",
        "scope": [
          "storage.type.primitive",
          "constant.language",
        ],
        "settings": {
          "foreground": "#bb83d1",
        }
      },
      {
        "name": "warnings",
        "scope": [
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
        ],
        "settings": {
          "foreground": "#b1a400",
          "fontStyle": "bold underline",
        }
      },
      {
        "name": "strings and primitives",
        "scope": [
          "meta.structure.dictionary.json string.quoted.double.json",
          "meta.structure.dictionary.json string.quoted.single.json",
        ],
        "settings": {
          "foreground": "#6fdbb2",
        }
      },
      {
        "name": "hashes and labels",
        "scope": [
          "constant.sha.git-rebase",
        ],
        "settings": {
          "foreground": "#00b8ff"
        }
      },
      {
        "name": "sematic: added/inserted",
        "scope": [
          "markup.inserted",
        ],
        "settings": {
          "foreground": "#447152",
        }
      },
      {
        "name": "semantic: deleted/removed",
        "scope": [
          "markup.deleted",
        ],
        "settings": {
          "foreground": "#5b1b1b",
        }
      },
      {
        "name": "semantic: changed/modified",
        "scope": [
          "markup.changed",
        ],
        "settings": {
          "foreground": "#43698d",
        }
      },
    ]

  },
}
